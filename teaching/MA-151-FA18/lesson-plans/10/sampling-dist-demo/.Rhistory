ns = seq(0, length(p) - 1)
mu.xbar = sum(p*ns/n)
sigma.xbar = sqrt(sum(p*(ns/n)^2) - mu.xbar^2)
if ((n <= 10) || (n == 30) || (n == 100)){
pdf(sprintf('figures/mean-%s-n%g.pdf', data.type, n))
