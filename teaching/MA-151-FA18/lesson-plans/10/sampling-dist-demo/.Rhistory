lines(xs/n, ps, lwd = 2)
if (n < 30){
segments(ns/n-0.5/n, 0, ns/n-0.5/n, p, lwd = 2)
segments(ns/n+0.5/n, 0, ns/n+0.5/n, p, lwd = 2)
abline(h = 0, lwd = 1)
}
abline(h = 0, lwd = 1)
abline(v = mu.xbar, col = 'blue', lwd = 2)
abline(v = c(mu.xbar - sigma.xbar, mu.xbar + sigma.xbar), lty = 2)
dev.off()
}
cat(sprintf("E[Sn] = %f\n", mu.xbar))
}
